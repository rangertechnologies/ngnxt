    "@angular/core": "~12.0.0",
    "@angular/forms": "~12.0.0",
    "@angular/material": "^12.0.2",
    "@angular/platform-browser": "~12.0.0",
    "@angular/platform-browser-dynamic": "~12.0.0",
    "@angular/router": "~12.0.0",
    "domsanitizer": "^0.2.2",
    "dotenv": "^8.6.0",
    "git-describe": "^4.0.4",
    "gulp": "^4.0.2",
    "gulp-cli": "^2.3.0",
    "gulp-file": "^0.4.0",
    "gulp-jsforce-deploy": "^1.1.2",
