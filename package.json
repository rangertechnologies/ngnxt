{
  "name": "ng-nxt",
  "version": "2.0.21",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "prebuild": "npm --no-git-tag-version version patch",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "devstatic": "ng build --prod --output-hashing none && gulp rm && gulp rmmap && gulp build-dev-static && gulp dev-deploy",
    "pkgstatic": "ng build --prod --output-hashing none && gulp rm && gulp rmmap && gulp build-prod-static && gulp pkg-deploy",
    "priostatic": "ng build --prod --output-hashing none && gulp rm && gulp rmmap && gulp build-prod-static && gulp prio-deploy",
    "postinstall": "node src/version.js"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~12.0.0",
    "@angular/cdk": "~12.0.0",
    "@angular/common": "~12.0.0",
